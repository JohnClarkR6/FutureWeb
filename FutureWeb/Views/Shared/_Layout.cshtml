@using System.Web.Optimization

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - FutureWeb </title>
     @Styles.Render("~/styles")
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="https://use.fontawesome.com/4ef72439e1.js"></script>
</head>
<body>

    
    <div class="navbar navbar-inverse navbar-fixed-top">
        <!--Start of nav Bar Top-->
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("FutureWeb", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
         
        </div>
    </div>     <!--End of navbar Top-->





    <div id="wrapper">

    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-2 sidebar">
                @Html.Action("Sidebar", "Layout")
            </div>
            <div id="body-content" class="col-lg-8">
                @RenderBody()
            </div>
            <div class="col-lg-2">
                 
              </div>
        </div>

      
    </div>
</div>

<form class="hidden" id="anti-forgery-form">
@Html.AntiForgeryToken()
</form>


    @Scripts.Render("~/scripts")
    @RenderSection("Scripts", false)

</body>
</html>